{{outlet}}
<div class="page-title">
  <h1 class="title">Registration Page</h1>
</div>
<div class="container">
  <div class="page">
    <form class="register">
      <input class="normal" type="email" name="email" placeholder="enter email">
      <input class="normal" type="password" name="password" placeholder="enter password">
      <input class="normal" type="password" name="confirmPassword" placeholder="enter confirm password">
      <input class="button normal" type="submit" value="Register">
    </form>
  </div>
</div>
