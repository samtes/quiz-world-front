<div class="page-title">
  <div class="nav-bar">
    <h1 class="title">{{title}}</h1>
    <div>
      <ul class="menu">
        {{#each sub in subTitles}}
          <li>{{#link-to sub.page class=sub.class}} {{sub.title}} {{/link-to}}</li>
        {{/each}}
        {{#if session.isAuthenticated}}
          <li><a class="active" {{action 'invalidateSession'}}>LOGOUT</a></li>
        {{/if}}
      </ul>
    </div>
  </div>
</div>
{{outlet}}
